{
  "name": "update-cloudflare-dns",
  "version": "0.3.3",
  "scripts": {
    "build": "tsc",
    "package": "yarn build; ncc build --source-map --license licenses.txt",
    "start": "yarn build; node .build/main.js",
    "dry": "yarn build; DRY_RUN=true node .build/main.js",
    "bump": "yarn version",
    "test": "jest --watch",
    "test-once": "jest",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@actions/core": "1.2.6",
    "@actions/github": "4.0.0",
    "cloudflare": "2.7.0",
    "dotenv": "8.2.0",
    "fp-ts": "2.9.5",
    "hjson": "3.2.2"
  },
  "devDependencies": {
    "@types/cloudflare": "2.7.0",
    "@types/hjson": "2.4.2",
    "@types/jest": "26.0.20",
    "@types/node": "14.14.32",
    "@typescript-eslint/parser": "4.8.1",
    "@vercel/ncc": "0.27.0",
    "jest": "26.6.3",
    "jest-circus": "26.6.3",
    "jest-extended": "0.11.5",
    "patch-package": "6.4.5",
    "ts-jest": "26.4.4",
    "typescript": "4.2.3"
  },
  "main": ".build/main.js",
  "repository": "https://github.com/pvinis/update-cloudflare-dns.git",
  "author": "Pavlos Vinieratos <pvinis@gmail.com>",
  "license": "MIT"
}
