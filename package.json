{
	"name": "update-cloudflare-dns",
	"version": "0.5.0",
	"license": "MIT",
	"repository": "https://github.com/pvinis/update-cloudflare-dns.git",
	"author": "Pavlos Vinieratos <pvinis@gmail.com>",
	"main": ".build/main.js",
	"engines": {
		"node": ">=16.0.0",
		"npm": ">=7.0.0"
	},
	"scripts": {
		"build": "tsc",
		"bump": "npm run package; git add dist; npm run version",
		"dry": "npm run build; DRY_RUN=true node .build/main.js",
		"package": "npm run build; ncc build --source-map --license licenses.txt",
		"postinstall": "patch-package",
		"start": "npm run build; node .build/main.js",
		"test": "jest --watch",
		"test-once": "jest"
	},
	"dependencies": {
		"@actions/core": "1.4.0",
		"@actions/github": "5.0.0",
		"cloudflare": "2.8.0",
		"dotenv": "10.0.0",
		"fp-ts": "2.11.1",
		"hjson": "3.2.2"
	},
	"devDependencies": {
		"@decadejs/eslint-config-base": "1.0.5",
		"@types/cloudflare": "2.7.5",
		"@types/hjson": "2.4.3",
		"@types/jest": "26.0.24",
		"@types/node": "16.4.8",
		"@typescript-eslint/eslint-plugin": "4.33.0",
		"@typescript-eslint/parser": "4.33.0",
		"@vercel/ncc": "0.29.0",
		"eslint": "7.32.0",
		"eslint-plugin-import": "2.23.4",
		"eslint-plugin-jest": "24.4.0",
		"jest": "27.0.6",
		"jest-extended": "0.11.5",
		"patch-package": "6.4.7",
		"prettier": "2.3.2",
		"prettier-eslint": "13.0.0",
		"prettier-plugin-package": "1.3.0",
		"ts-jest": "27.0.4",
		"ts-node": "10.1.0",
		"typescript": "4.3.5"
	}
}
